   "source": [
    "import numpy as np\n",
    "\n",
    "a = np.array([1,1,1])\n",
    "b = np.array([2,2,2])\n",
    "\n",
    "c = np.vstack((a,b))  # vertical stack\n",
    "d = np.hstack((a,b))  # horizontal stack\n",
    "print(c)  \n",
    "print(d)  \n",
    "\n",
    "print(a.shape)\n",
    "print(c.shape)\n",
    "print(d.shape)"
   ]
  }
